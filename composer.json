{
    "name": "worzy/mailzap",
    "description": "Package for mass sending email through various providers",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Gareth Drew",
            "email": "hello@garethdrew.me"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "guzzlehttp/guzzle": "~6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "larapack/dd": "^1.1"
    },
    "extra": {
        "laravel": {
            "providers": [
                "worzy\\mailzap\\MailzapServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Worzy\\Mailzap\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Worzy\\Mailzap\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
